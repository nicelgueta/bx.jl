{
    "manifest_version": 3,
    "name": "Bx",
    "version": "1.0",
    "description": "Inject bx tags into a webpage for training an auto-scraper",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "js/service.js"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128":"images/icon.png"
    }
}

